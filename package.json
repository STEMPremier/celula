    "webpack-cli": "^3.3.9"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.10.2"
  },
  "scripts": {
    "build": "yarn run prebuild && yarn run build:components && yarn run build:styleguide",
    "build:components": "NODE_ENV=production webpack -p",
    "build:icons": "svg-sprite-generate -d src/components/icon/icons -o src/components/icon/icons/icons.svg",
    "build:styleguide": "build-storybook",
    "check": "yarn run lint:check && yarn run pretty:check",
    "lint:check": "eslint \"**/*.{js,jsx}\"",
    "lint:fix": "eslint --fix \"**/*.{js,jsx}\"",
    "optimize:icons": "node src/utils/svgOptimize.js",
    "prebuild": "yarn run pretty:fix && yarn lint:fix",
    "pretty:check": "prettier --check \"**/*.{js,jsx}\"",
    "pretty:fix": "prettier --write \"**/*.{js,jsx}\"",
    "start": "start-storybook -p 6006",
    "test": "NODE_ENV=test jest --verbose --coverage",
    "test:watch": "NODE_ENV=test jest --verbose --watch",
    "test:debug": "NODE_ENV=test node --inspect node_modules/.bin/jest --runInBand --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/STEMPremier/tallo-ui-kit.git"
  },
  "keywords": [
    "celula",
    "react",
    "ui-kit",
    "styleguide",
    "user-interface",
    "components",
    "style",
    "documentation"
  ],
  "author": "Tallo Inc., Engineering",
  "engines": {
    "node": "10.x",
    "yarn": "1.x"
  },
