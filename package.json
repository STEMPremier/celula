    "@storybook/addons": "^5.3.13",
    "@storybook/react": "^5.3.13",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-react-docgen": "^4.1.0",
    "cross-env": "^5.2.1",
